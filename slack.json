{
  "display_information": {
      "name": "Guy Veut",
      "description": "A reward system bot",
      "background_color": "#000000"
  },
  "features": {
      "bot_user": {
          "display_name": "Guy Veut",
          "always_online": true
      },
      "slash_commands": [
          {
              "command": "/guyveut",
              "url": "<<REPORTS_ENDPOINT>>/api/reports",
              "description": "Show leaderboard of cookies holders",
              "should_escape": false
          }
      ]
  },
  "oauth_config": {
      "scopes": {
          "bot": [
              "app_mentions:read",
              "channels:history",
              "reactions:write",
              "chat:write",
              "commands"
          ]
      }
  },
  "settings": {
      "event_subscriptions": {
          "request_url": "<<BANKER_ENDPOINT>>/api/message",
          "bot_events": [
              "message.channels"
          ]
      },
      "interactivity": {
          "is_enabled": true,
          "request_url": "<<REPORTS_ENDPOINT>>/api/action"
      },
      "org_deploy_enabled": false,
      "socket_mode_enabled": false,
      "token_rotation_enabled": false
  }
}
